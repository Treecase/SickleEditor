[Desktop Entry]
Type=Application
Version=1.5
Name=@PROJECT_NAME@
Comment=@PROJECT_DESCRIPTION@
Icon=@APPLICATION_ID@
Exec=sickle %f
# There aren't official MIME types for the goldsrc .map or .rmf formats, so I just made these up based on the existing vnd.valve.source.* types.
MimeType=application/vnd.valve.goldsrc.map;application/vnd.valve.goldsrc.rmf
Categories=Graphics;3DGraphics
Keywords=goldsrc;map